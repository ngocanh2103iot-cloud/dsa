#include <iostream>
using namespace std;
int n,a[10000],ok,k;
void ktao(){
   a[1]=0;
   for(int i=2;i<=n;i++) a[i]=1;
}
void in(){
    for(int i=1;i<=n;i++)
        if(a[i]==0) cout<<8;
        else cout<<6;
    cout<<endl;
}
bool check(){
    for(int i=1;i<=n;i++) if(a[i]==a[i+1]&&a[i]==0) return false;
    for(int i=1;i<=n;i++) if(a[i]==a[i+1]&&a[i]==1&&a[i-1]==a[i]&&a[i+2]==a[i]) return false;
    return true;
}
void sinh(){
   int i=n-1;
   while(i>=1&&a[i]==0){
    a[i]=1;
    i--;
   }
   if(i==0) ok=0;
   else a[i]=0;
}
int main()
{
    cin>>n ;ok=1;
    ktao();
    while(ok){
        if(check()) in();
        sinh();
    }
    //cout<<endl;
    return 0;
}
