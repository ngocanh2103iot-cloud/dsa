#include <iostream>
using namespace std;
int main()
{
    int i,n,j,t;
    cin>>t;
    string s;
    while(t--){
        cin>>s;
        i=s.length()-1;
        while(i>=0&&s[i]=='1'){
            s[i]='0';
            i--;
        }
        if(i>=0) s[i]='1';
        cout<<s<<endl;
    }
    return 0;
}
