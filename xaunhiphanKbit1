#include <iostream>
using namespace std;
int n,a[10000],ok,k;
void ktao(){
   for(int i=1;i<=n;i++) a[i]=0;
}
void in(){
    int dem=0;
   for(int j=1;j<=n;j++)
        if(a[j]==1) dem++;
   if(dem==k){
   for(int i=1;i<=n;i++)
        cout<<a[i];
    cout<<endl;
   }
}
void sinh(){
   int i=n;
   while(i>=1&&a[i]==1) {
    a[i]=0;
    i--;
   }
   if(i==0) ok=0;
   else a[i]=1;
}
int main()
{
    int t;
    cin>>t;
    while(t--){
    cin>>n>>k ;ok=1;
    ktao();
    while(ok){
         in();
        sinh();
    }
    //cout<<endl;
    }
    return 0;
}
